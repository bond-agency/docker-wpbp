#
# * Custom Settings
#
[mysqld]
max_connections         = 50    # default: 100
thread_cache_size       = 50    # default: 128
key_buffer_size         = 64M   # default: 128M
query_cache_limit       = 256K  # default: 128K
query_cache_size        = 64M   # default: 64M
innodb_buffer_pool_size = 128M  # default: 256M